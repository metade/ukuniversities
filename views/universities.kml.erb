<?xml version='1.0' encoding='UTF-8'?>
<kml xmlns='http://www.opengis.net/kml/2.2'>
  <% @universities.each do |university| %>
    <Placemark>
      <name><%= university.name %></name>
      <Point>
        <coordinates><%= university.latitude %>,<%= university.longitude %></coordinates>
      </Point>
    </Placemark>
  <% end %>
</kml>
